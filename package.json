{
    "name": "@ayaka209/claude-context",
    "version": "0.2.4",
    "description": "Enhanced semantic code search with advanced embedding support and external project management",
    "private": true,
    "scripts": {
        "build": "pnpm -r --filter='!./examples/*' build && pnpm -r --filter='./examples/*' build",
        "build:core": "pnpm --filter @ayaka209/claude-context-core build",
        "build:vscode": "pnpm --filter semanticcodesearch compile",
        "build:mcp": "pnpm --filter @ayaka209/claude-context-mcp build",
        "dev": "pnpm -r dev",
        "dev:core": "pnpm --filter @ayaka209/claude-context-core dev",
        "dev:vscode": "pnpm --filter semanticcodesearch watch",
        "dev:mcp": "pnpm --filter @ayaka209/claude-context-mcp dev",
        "clean": "pnpm -r clean",
        "lint": "pnpm -r lint",
        "lint:fix": "pnpm -r run lint:fix",
        "typecheck": "pnpm -r run typecheck",
        "release:all": "pnpm build && pnpm --filter @ayaka209/claude-context-core publish --access public --no-git-checks && pnpm --filter @ayaka209/claude-context-mcp publish --access public --no-git-checks",
        "release:core": "pnpm --filter @ayaka209/claude-context-core... run build && pnpm --filter @ayaka209/claude-context-core publish --access public --no-git-checks",
        "release:mcp": "pnpm --filter @ayaka209/claude-context-mcp... run build && pnpm --filter @ayaka209/claude-context-mcp publish --access public --no-git-checks",
        "release:vscode": "pnpm --filter @ayaka209/claude-context-core build && pnpm --filter semanticcodesearch run webpack && pnpm --filter semanticcodesearch release",
        "example:basic": "pnpm --filter claude-context-basic-example start",
        "benchmark": "node scripts/build-benchmark.js",
        "test:embedding": "node scripts/test-embedding.js",
        "test:embedding:quick": "bash scripts/test-embedding.sh",
        "test:embedding:full": "bash scripts/test-embedding.sh --full-test",
        "test:embedding:template": "bash scripts/test-embedding.sh --template",
        "test:custom": "node scripts/test-custom-embedding.js",
        "test:custom:shell": "bash scripts/test-custom-embedding.sh",
        "test:v4-2048": "node scripts/test-text-embedding-v4-2048.js",
        "index:project": "node scripts/index-project.js",
        "index:sync": "node scripts/sync-index.js",
        "check:index": "node scripts/check-index-status.js",
        "check:milvus": "node scripts/check-milvus-collections.js",
        "debug:vectordb": "node scripts/debug-vectordb.js",
        "logs": "node scripts/view-logs.js",
        "logs:list": "node scripts/view-logs.js --list",
        "logs:clean": "node scripts/view-logs.js --clean",
        "index:stop": "node scripts/stop-indexing.js",
        "index:status": "node scripts/indexing-status.js",
        "manual-query": "node scripts/manual-query.js",
        "manual-query:interactive": "node scripts/manual-query-interactive.js",
        "query": "node scripts/manual-query-interactive.js"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^8.31.1",
        "@typescript-eslint/parser": "^8.31.1",
        "assert": "^2.1.0",
        "browserify-zlib": "^0.2.0",
        "eslint": "^9.25.1",
        "https-browserify": "^1.0.0",
        "os-browserify": "^0.3.0",
        "rimraf": "^6.0.1",
        "stream-http": "^3.2.0",
        "tree-sitter-cli": "^0.25.6",
        "typescript": "^5.8.3",
        "url": "^0.11.4",
        "vm-browserify": "^1.1.2"
    },
    "engines": {
        "node": ">=20.0.0",
        "pnpm": ">=10.0.0"
    },
    "license": "MIT",
    "author": "ayaka209",
    "repository": {
        "type": "git",
        "url": "https://github.com/ayaka209/claude-context.git"
    },
    "homepage": "https://github.com/ayaka209/claude-context#readme",
    "bugs": {
        "url": "https://github.com/ayaka209/claude-context/issues"
    },
    "keywords": ["claude", "context", "semantic-search", "mcp", "vector-database", "ai", "code-search", "fork"],
    "dependencies": {
        "dotenv": "^16.5.0"
    }
}